{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "rootDir": "./",
      "baseUrl": "./",
      "paths": {"app/*": ["./src/app/*"],
                "cp/*": ["./src/cp/*"],
                "entities/*": ["./src/entities/*"],
                "hooks/*": ["./src/hooks/*"],
                "types/*": ["src/@types/*"],
                "navigation/*": ["src/navigation/*"],
                "screens/*": ["src/screens/*"],
                "states/*": ["src/states/*"],
                },
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "strictPropertyInitialization": false,                 
    }
  }